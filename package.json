{
	"name": "svelte5-single-page-application",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"biome:format": "npx @biomejs/biome format --write src",
		"biome:lint": "npx @biomejs/biome lint src",
		"biome:lint:autofix": "npx @biomejs/biome lint --write src",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@chromatic-com/storybook": "^3.2.3",
		"@octokit/request": "^9.1.3",
		"@storybook/addon-essentials": "^8.4.7",
		"@storybook/addon-interactions": "^8.4.7",
		"@storybook/addon-svelte-csf": "^5.0.0-next.21",
		"@storybook/blocks": "^8.4.7",
		"@storybook/svelte": "^8.4.7",
		"@storybook/sveltekit": "^8.4.7",
		"@storybook/test": "^8.4.7",
		"@sveltejs/adapter-auto": "^3.3.1",
		"@sveltejs/kit": "^2.15.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"autoprefixer": "^10.4.20",
		"msw": "^2.6.5",
		"neverthrow": "^8.1.1",
		"storybook": "^8.4.7",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.7",
		"tailwindcss": "^3.4.9",
		"typescript": "^5.0.0",
		"vite": "^5.0.3",
		"vitest": "^2.0.4"
	},
	"type": "module"
}
