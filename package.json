{
  "name": "svelte5-single-page-application",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "biome:format": "npx @biomejs/biome format --write src",
    "biome:lint": "npx @biomejs/biome lint src",
    "biome:lint:autofix": "npx @biomejs/biome lint --write src",
    "test:unit": "vitest",
    "test": "npm run test:unit -- --run",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@chromatic-com/storybook": "^3.2.2",
    "@octokit/request": "^9.1.3",
    "@storybook/addon-essentials": "^8.4.1",
    "@storybook/addon-interactions": "^8.4.1",
    "@storybook/addon-svelte-csf": "^5.0.0-next.10",
    "@storybook/blocks": "^8.4.1",
    "@storybook/svelte": "^8.4.1",
    "@storybook/sveltekit": "^8.4.1",
    "@storybook/test": "^8.4.1",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.12.1",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "autoprefixer": "^10.4.20",
    "msw": "^2.6.5",
    "neverthrow": "^8.1.1",
    "storybook": "^8.4.1",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.7",
    "tailwindcss": "^3.4.9",
    "typescript": "^5.0.0",
    "vite": "^5.0.3",
    "vitest": "^2.0.4"
  }
}
